<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>dds-utn</groupId> <!-- agrupa todos los artefactos que posee un organizacion  -->
  <artifactId>Ejercicio_GranViaje</artifactId> <!-- nombre de proyecto -->
  <version>0.0.1-SNAPSHOT</version>
  
  <!-- Dependencias -->
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
		</dependency>
	</dependencies>
	
	<!-- Version Java 8 -->
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>